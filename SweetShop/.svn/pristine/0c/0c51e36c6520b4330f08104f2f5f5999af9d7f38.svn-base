	div.cutting_line_btm  { position:absolute; left:0; top:-6px; width:100%; height:10px; background:url('../images/common/cutting_line_btm.png') repeat-x left center;  background-size:10px 3px; }

	div.shop_detail div.ribbon { position:absolute; width:27px; height:44px; left:10px; top:0; }
