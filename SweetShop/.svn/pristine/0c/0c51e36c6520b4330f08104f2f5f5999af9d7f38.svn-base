	div.favorite img { width:100%; height:auto; vertical-align:top }

	div.shop_detail > ul.cont { width:100%; padding-top:1px; padding-bottom:10px}
	div.shop_detail > ul.cont:after {content:"";display:block;clear:both;}
	div.shop_detail > ul.cont > li.title { width:100%; padding:33px 15% 0;  text-overflow:ellipsis; overflow:hidden; white-space:nowrap; margin-bottom:5px;text-align:center; font-size:20px; font-weight:700;}
	div.shop_detail > ul.cont > li.title a span {display:block ; white-space:nowrap; overflow:hidden; text-overflow:ellipsis; font-size:20px; }
	div.shop_detail > ul.cont > li.title a span.notcolor { font-size:14px; color:#777786; font-weight:normal; line-height:14px }
	div.shop_detail > ul.cont > li.photo {width:100%; padding:0 15%;}
	div.shop_detail > ul.cont > li.photo img { width:100%; height:auto; }
	div.shop_detail > ul.cont > li.memo { text-align:center; margin-top:16px; color:#777786; font-size:14px; line-height:130%; padding:0 3%}
		div.shop_detail > ul.cont.con_store > li.memo { padding:0 7%}

	div.shop_detail > ul.cont > li.infomation > ul {width:100%; padding:15px 7% 10px; }
	div.shop_detail > ul.cont > li.infomation > ul:after {content:"";display:block;clear:both;} 
	div.shop_detail > ul.cont > li.infomation > ul > li { float:left; }
	div.shop_detail > ul.cont > li.infomation > ul > li:first-child { width:62%; text-align:left; }
	div.shop_detail > ul.cont > li.infomation > ul > li:first-child img { width:22%;  max-width:31px; height:auto; margin-right:2px;}
	div.shop_detail > ul.cont > li.infomation > ul > li:last-child { width:38%; text-align:right; font-size:13px; color:#a9a9b3; font-weight:300; }
	div.shop_detail > ul.cont > li.infomation > ul > li:last-child p {height:13px; line-height:13px; vertical-align:top; margin-bottom:2px; }
	div.shop_detail > ul.cont > li.infomation > ul > li:last-child img { width:100%; max-width:79px; height:auto; }

	div.shop_detail > div.shop_info_basic { width:100%; padding:6px 7%; }
	/* div.shop_detail > div.shop_info_basic:after {content:"";display:block;clear:both;} */
	div.shop_detail > div.shop_info_basic > dl { margin-bottom:10px; white-space:nowrap; overflow:hidden; text-overflow:ellipsis; display:table; width:100%; table-layout:fixed; text-overflow:hidden;}
	div.shop_detail > div.shop_info_basic > dl:after {content:"";display:block;clear:both;}
	div.shop_detail > div.shop_info_basic > dl > dt,
	div.shop_detail > div.shop_info_basic > dl > dd {font-size:13px; line-height:20px; display:table-cell; }
	div.shop_detail > div.shop_info_basic > dl > dt { width:84px !important; color:#777786; }
	div.shop_detail > div.shop_info_basic > dl > dd { color:#999;}
	div.shop_detail > div.shop_info_basic > dl > dd a, div.shop_detail > div.shop_info_basic > dl > dd a:hover, div.shop_detail > div.shop_info_basic > dl > dd a:active{display:block; font-size:13px; color:#999;}
	div.shop_detail > div.shop_info_basic > dl > dd.underline { text-decoration:underline; }

	div.shop_detail .shop_star {text-align:center; padding-top:14px}
	div.shop_detail > div.shop_info_basic img {dispaly:inline-block; width:14px; height:auto; margin-right:7px;}






