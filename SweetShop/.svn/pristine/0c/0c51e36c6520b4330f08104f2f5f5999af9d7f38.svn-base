@charset "utf-8";


#wrap { position:relative; width:100%; padding:0 3%; }
#wrap_wide { position:relative; width:100%; padding-bottom:10%  }


/**** 상단 기본 구성 ***/

header { width:100%; }
header:after {content:"";display:block;clear:both;} 
header ul {}
header li { float:left; width:20%; height:50px; line-height:50px;  text-align:center;  }
header li a { display:block;  }
header li:nth-child(1) { text-align:left;}
header li:nth-child(1) a { color:#ff8b72; font-size:16px; }
header li:nth-child(1) a img {width:22px; height:auto;}
header li:nth-child(2) { width:60%; color:#484852; font-weight:500; font-size:17px;}
header li:nth-child(2) a img { width:72px; height:auto;}
header li:nth-child(3) { text-align:right;}
header li:nth-child(3) a { color:#ff8b72; font-weight:500; font-size:20px;}
header li:nth-child(3) a img {width:22px; height:auto;}

/**** 상단 기본 구성 ***/

header.sub { width:100%; padding:0 3%; }
header.sub:after {content:"";display:block;clear:both;} 
header.sub ul {}
header.sub li { float:left; width:25%; height:50px; line-height:50px;  text-align:center;  }
header.sub li a { display:block;  }
header.sub li:nth-child(1) { text-align:left;}
header.sub li:nth-child(2) { width:50%;}
header.sub li:nth-child(3) { text-align:right;}
header.sub.pos_fixed{ position:fixed; top:0; left:0; z-index:2; background:#ffffff; }

section.sub_content_wrap { width:100%; padding:0 5%; height:100%;}
section.sub_content_wrap_wide { width:100%; padding-bottom:0px;}
section.sub_content_wrap_wide.pos_fixed{ position:fixed; top:0; left:0; z-index:1; background:#ffffff; }

section.sub_content_wrap:after {content:"";display:block;clear:both;} 
section.sub_content_wrap_wide:after {content:"";display:block;clear:both;} 


/* 좌측 판넬 메뉴 */
div.left_menu_title { background:#ff8b72; height:70px; line-height:65px; padding-left:15px;}
div.left_gnb {}
div.left_gnb:after {content:"";display:block;clear:both;} 
div.left_gnb ul { border-bottom:1px solid #cfcfd4; padding:25px 0 20px;}
div.left_gnb ul:after {content:"";display:block;clear:both;} 
div.left_gnb ul li { float:left; width:33.3333%; }
div.left_gnb ul li a { display:block; text-align:center; font-weight:300; color:#a9a9b3;}
div.left_gnb ul li a p { margin-bottom:7px; border-right:1px solid #cfcfd4; }
div.left_gnb ul li:last-child a p  {border-right:none; }


div.left_gnb_list {}
div.left_gnb_list:after {content:"";display:block;clear:both;} 
div.left_gnb_list ul { }
div.left_gnb_list ul:after {content:"";display:block;clear:both;} 
div.left_gnb_list ul li { width:100%;  border-bottom:1px solid #cfcfd4;}
div.left_gnb_list ul li a { padding:10px 18px;display:block; text-align:left; font-weight:300; color:#a9a9b3;}
div.left_gnb_list ul li a:focus { color:#ff8b72; }
div.left_gnb_list ul li a img { margin-right:7px; }

div.left_copyright { height:40px; padding-left: 15px; padding-top:10px; font-size:12px; color:#777786; }



nav.gnb { margin:15px 0;}
nav.gnb:after {content:"";display:block;clear:both;} 
.swiper-container {width: 100%;  }
.swiper-container:after {content:"";display:block;clear:both;} 
.swiper-slide { text-align: center; font-size: 18px;   
        display: -webkit-box;
        display: -ms-flexbox;
        display: -webkit-flex;
        display: flex;
        -webkit-box-pack: center;
        -ms-flex-pack: center;
        -webkit-justify-content: center;
        justify-content: center;
        -webkit-box-align: center;
        -ms-flex-align: center;
        -webkit-align-items: center;
        align-items: center;
    }
.gnbcolor a { display:block; font-weight:500; color:#a9a9b3; height:35px;/* */ }
.nav_focus { border-bottom:2px solid #ff8b72; }
.nav_focus a { color:#ff8b72; }

