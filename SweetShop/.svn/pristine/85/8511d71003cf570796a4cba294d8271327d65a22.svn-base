<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

	<mapper namespace="com.sweetk.web.admin.mapper.CodeMapper">

	<select id="CodeList" parameterType="CodeVo" resultType="map">

		select *
		from code_mst
		where code_type = #{code_type}

	</select>

	</mapper>
