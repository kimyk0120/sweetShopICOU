<script>
	$(function(){
		function pop_layer( show ){
			if( show ){
				$("#overlay_t").show();
				setTimeout(function(){
					show_pop($("#popup_layer_insert"));		
				}, 100 );
				$("body").on("touchmove", function(e){e.preventDefault()});
			}else{
				$('#popup_layer_insert, #overlay_t').hide(); 	
				$("body").off("touchmove");
			}
		}   	
	   
	   $('#overlay_t, .close').click(function(e){ 
		   e.preventDefault(); 
		   pop_layer(false);
	   }); 	
	   
		function show_pop( obj ){
			obj.show();				
			var popH = obj.innerHeight();
			var popW = obj.innerWidth();
			var winW = $(window).width();
			var winH = $(window).height();
			var posW = (winW-popW)/2;
			var posY = (winH-popH)/2;
			obj.css({"left":posW, "top":posY});	   
		}
	   
	   pop_layer(true);		
		
		function map_show(){
			var mapContainer = document.getElementById('location_map'), // 지도를 표시할 div 
				mapOption = { 
					center: new daum.maps.LatLng(37.3901724,126.95388200000002), // 지도의 중심좌표
					level: 3 // 지도의 확대 레벨
				};
			
			var map = new daum.maps.Map(mapContainer, mapOption);
			
			// 마커가 표시될 위치입니다 
			var markerPosition  = new daum.maps.LatLng(37.3901724,126.95388200000002); 
			
			// 마커를 생성합니다
			var marker = new daum.maps.Marker({
				position: markerPosition
			});
			
			// 마커가 지도 위에 표시되도록 설정합니다
			marker.setMap(map);
			
		}		
		map_show();
		
		var currentShow = false;
		//상세설명
		$(".shoview_detail_text .arrow a").on("click", function(){		
			if($(this).hasClass("on")){
				$(this).removeClass("on");
				$(".shoview_detail_text .cont").addClass("over_2line");
			}else{
				$(this).addClass("on");
				$(".shoview_detail_text .cont").removeClass("over_2line");
			}
			
		});
		
		//위치정보
		$(".shoview_detail_map .arrow a").on("click", function(){		
			if($(this).hasClass("on")){
				$("#location_map").hide();			
				$(this).removeClass("on");
			}else{
				$("#location_map").show();
				if( !currentShow ){
					map_show();
				}
				$(this).addClass("on");
				currentShow = true;	
			}
			
		});
		
		 
		
	});
	
	
</script>

